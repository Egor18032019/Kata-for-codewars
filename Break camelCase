// complete the function
function solution(string) {
    const array = string.split("") // загоняю всё в массив
   let result =[] 
   for (let i = 0; i < array.length; i++) {
    if (array[i].toUpperCase() === array[i]) {
      result.push(" ") // если с большой буквы то закидываю пробел
  }
    result.push(array[i]);
  }
        return result.join("")
}
